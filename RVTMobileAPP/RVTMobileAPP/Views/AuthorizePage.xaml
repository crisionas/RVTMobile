<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RVTMobileAPP.Views.AuthorizePage">
    <ContentPage.Content>
        <StackLayout Margin="30" Spacing="20" VerticalOptions="Center">
            <Image Source="RVTLogo.png"></Image>
            <Button  Text="Logare" Clicked="Button_OnClicked"/>
            <Button  Text="Înregistrare"  Command="{Binding LoginCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>